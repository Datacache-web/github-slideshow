<?xml version="1.0" encoding="utf-8"?>

<odoo>
 <data>
    
    <!-- Inherited For Add Product License Tab -->
	<record id="inherited_view_partner_product_license_form" model="ir.ui.view">
		    <field name="name">res.partner.form.inherited</field>
		    <field name="model">res.partner</field>
		    <field name="inherit_id" ref="base.view_partner_form"/>
		    <field name="arch" type="xml">
		        <xpath expr="//page[@name='internal_notes']" position='after'>
		    	  <page name='product_license' string="Product License">
		    	     <group>
		    	       <field name="renewal_date"/>
		    	       <field name="user_limit"/>
		    	       <field name="expiry_date"/>
		    	       <field name="product_code"/>
		    	       <field name="active_box"/>
		    	       <field name="encryption_key"/>
		    	       <field name="validity_status"/>
		    	     </group>
		    	  </page>
		        </xpath>
		    </field>
   </record>

 </data>
</odoo>